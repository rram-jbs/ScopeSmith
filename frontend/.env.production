# Production API configuration - will be replaced during deployment
VITE_API_BASE_URL=https://<api-id>.execute-api.<region>.amazonaws.com/prod

# Feature flags
VITE_ENABLE_TEMPLATE_UPLOAD=true

# Configuration
VITE_MAX_FILE_SIZE=5242880 # 5MB in bytes
VITE_POLLING_INTERVAL=2000 # 2 seconds
VITE_MAX_POLLING_TIME=300000 # 5 minutes